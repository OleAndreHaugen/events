	"content": [
		"==OBJECT STRING==",
		"const { In, Like } = operators;\r",
		"\r",
		"let where = {};\r",
		"\r",
		"if (req.query.filter) where.title = Like(\"%\" + req.query.filter + \"%\");\r",
		"\r",
		"if (req.query.type) {\r",
		"    const type = req.query.type.split(\",\");\r",
		"    where.type = In(type);\r",
		"}\r",
		"\r",
		"if (req.query.category) {\r",
		"    const category = req.query.category.split(\",\");\r",
		"    where.type = In(category);\r",
		"}\r",
		"\r",
		"if (req.query.targetgroup) {\r",
		"    const targetgroup = req.query.targetgroup.split(\",\");\r",
		"    where.targetgroup = In(targetgroup);\r",
		"}\r",
		"\r",
		"const activity = await entities.event_activity.find({ where: where });\r",
		"result.data = activity;\r",
		"complete();"
	],
